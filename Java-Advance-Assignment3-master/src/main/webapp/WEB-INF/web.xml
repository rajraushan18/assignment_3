  	<display-name>RegisterUser</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.javaAdvance.assignment3.servlet.RegisterUser</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Welcome</servlet-name>
  	<display-name>Welcome</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.javaAdvance.assignment3.servlet.Welcome</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>ImageUtilityServlet</servlet-name>
    <jsp-file>/ImageUtility.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ForgetPass</servlet-name>
  	<url-pattern>/ForgetPass</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ImageDelete</servlet-name>
  	<url-pattern>/ImageDelete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ImageEdit</servlet-name>
  	<url-pattern>/ImageEdit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
