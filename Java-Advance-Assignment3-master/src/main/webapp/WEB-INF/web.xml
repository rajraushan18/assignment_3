<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ForgetPass</servlet-name>
  	<display-name>ForgetPass</display-name>
  	<description>gets called when 'forgot password' is clicked and returns password!!</description>
  	<servlet-class>com.nagarro.javaAdvance.assignment3.servlet.ForgetPass</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ImageDelete</servlet-name>
  	<display-name>ImageDelete</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.javaAdvance.assignment3.servlet.ImageDelete</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ImageEdit</servlet-name>
  	<display-name>ImageEdit</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.javaAdvance.assignment3.servlet.ImageEdit</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ImageSave</servlet-name>
  	<display-name>ImageSave</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.javaAdvance.assignment3.servlet.ImageSave</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
