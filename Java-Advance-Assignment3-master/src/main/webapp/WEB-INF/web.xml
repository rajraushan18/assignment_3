  	<url-pattern>/RegisterUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Welcome</servlet-name>
  	<url-pattern>/Welcome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ImageUtilityServlet</servlet-name>
    <url-pattern>/ImageUtility</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>loginPage.html</welcome-file>
